{
    "name": "widget-template-vue",
    "version": "1.0.0",
    "description": "Widget Template using Vue.js",
    "main": "index.html",
    "config": {
        "publicPath": null
    },
    "scripts": {
        "postinstall": "node webpack/postInstall.js",
        "test": "mocha test/e2e/bootstrap.js --recursive test --timeout 10000",
        "debugTest": "mocha --inspect-brk test/bootstrap.js --recursive test --timeout 10000",
        "build": "webpack --config ./webpack/webpack.config.prod.js",
        "buildS3": "webpack --config ./webpack/webpack.config.prod.s3.js",
        "start": "webpack-dev-server --config ./webpack/webpack.config.dev.js --open",
        "startS3": "webpack-dev-server --config ./webpack/webpack.config.dev.s3.js",
        "debugS3": "node --inspect node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./webpack/webpack.config.dev.s3.js",
        "lint": "eslint src/**/*.js",
        "lintFix": "eslint src/**/*.js --fix",
        "lintMark": "markdownlint --config .markdownlint.json --ignore ./node_modules/** ./**/*.md",
        "lintMarkFix": "markdownlint --fix --config .markdownlint.json --ignore ./node_modules/** ./**/*.md"
    },
    "repository": {
        "type": "git",
        "url": "git@domain.com:project/repo.git"
    },
    "proxy": "https://localhost:8081",
    "author": "widget-lab",
    "license": "MIT",
    "devDependencies": {
        "@auth0/s3": "1.0.0",
        "@babel/core": "7.7.5",
        "@babel/preset-env": "7.7.6",
        "@mdi/font": "4.7.95",
        "babel-loader": "8.0.6",
        "babel-polyfill": "^6.26.0",
        "clean-webpack-plugin": "3.0.0",
        "copy-webpack-plugin": "5.1.1",
        "css-loader": "3.4.0",
        "deepmerge": "4.2.2",
        "eslint": "6.7.2",
        "eslint-config-prettier": "6.7.0",
        "eslint-config-vuetify": "0.4.1",
        "eslint-loader": "3.0.3",
        "eslint-plugin-html": "6.0.0",
        "eslint-plugin-json": "2.0.1",
        "eslint-plugin-mocha": "6.2.2",
        "eslint-plugin-vue": "6.0.1",
        "express": "4.17.1",
        "file-loader": "5.0.2",
        "highlight.js": "9.17.1",
        "markdownlint": "0.18.0",
        "markdownlint-cli": "0.20.0",
        "material-design-icons-iconfont": "5.0.1",
        "mermaid": "8.4.4",
        "mocha": "6.2.2",
        "puppeteer": "2.0.0",
        "raw-loader": "4.0.0",
        "remarkable": "2.0.0",
        "sass": "1.23.7",
        "sass-loader": "8.0.0",
        "style-loader": "1.0.2",
        "vue": "2.6.11",
        "vue-loader": "15.8.3",
        "vue-template-compiler": "2.6.11",
        "vuetify-loader": "1.4.3",
        "vuex": "3.1.2",
        "webpack": "4.41.3",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "3.3.10",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "4.2.2"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.4.0",
        "@fortawesome/fontawesome-svg-core": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "ag-grid-community": "^29.3.5",
        "ag-grid-enterprise": "^29.3.5",
        "ag-grid-vue": "^29.3.5",
        "axios": "^1.4.0",
        "file-saver": "^2.0.5",
        "vue-awesome-notifications": "^3.1.0",
        "vue-drag-drop": "^1.1.4",
        "xlsx": "^0.18.5"
    }
